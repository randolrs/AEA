<% if user_signed_in? %>

	<% if current_user.basic_profile %>

		<%= render partial: 'pages/home_user_dashboard', locals: {source: @source} %>

	<%else%>

		<%= render partial: 'pages/home_user_no_profile', locals: {source: @source} %>

	<%end%>

<% else %>

	<%= render partial: 'pages/home_no_user', locals: {source: @source} %>

<% end %>