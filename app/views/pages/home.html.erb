<% if user_signed_in? %>

	<% if current_user.basic_profile %>

		<%= render partial: 'pages/home_user_dashboard', locals: {source: @source} %>

	<%else%>

		<%= render partial: 'pages/home_user_no_profile', locals: {source: @source} %>

	<%end%>

<% else %>

	<%= render partial: 'pages/home_no_user', locals: {source: @source} %>

<% end %>

<%= render partial: 'questions/return_question_details' %>

<% if @question_show %>

	<%= render partial: 'pages/question_show', locals:{source: @source, show_question: @question_show} %>

<% elsif @show_user_question %>

	<%= render partial: 'questions/response_form', locals:{source: @source, question: @show_user_question} %>

<% end %>